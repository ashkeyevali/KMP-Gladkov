CREATE TABLE GAMES (
    game_id TEXT NOT NULL,
    game_title TEXT NOT NULL,
    game_version TEXT NOT NULL,
    game_size TEXT NOT NULL,
    PRIMARY KEY (game_id)
);

getAllGames:
SELECT * FROM GAMES;

insertGames:
INSERT OR REPLACE INTO GAMES (game_id, game_title, game_version, game_size) VALUES (?, ?, ?, ?);


